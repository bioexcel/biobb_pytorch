{
    "_id": "biobb_pytorch",
    "desc": "Biobb_pytorch is a BioBB category for creating and training Machine Learning and Deep Learning models using PyTorch. It allows building, training, and evaluating Molecular Dynamics AutoEncoders (MDAE) and other PyTorch-based models for analyzing molecular dynamics trajectories.",
    "github": "https://github.com/bioexcel/biobb_pytorch",
    "readthedocs": "https://biobb-pytorch.readthedocs.io/en/latest/",
    "conda": "https://anaconda.org/bioconda/biobb_pytorch",
    "docker": "https://quay.io/biocontainers/biobb_pytorch:5.2.0--pyhad2cae4_0",
    "singularity": "https://depot.galaxyproject.org/singularity/biobb_pytorch:5.2.0--pyhad2cae4_0",
    "version": "5.2.0",
    "rest": true,
    "tools": [
        {
            "block": "BuildModel",
            "tool": "build_model",
            "desc": "Build a PyTorch autoencoder model (MDAE) from input statistics and hyperparameters.",
            "exec": "buildModel",
            "docs": "https://biobb-pytorch.readthedocs.io/en/latest/mdae.html#module-biobb_pytorch.mdae.build_model",
            "rest": true
        },
        {
            "block": "MDFeaturePipeline",
            "tool": "mdfeaturizer",
            "desc": "Obtain Molecular Dynamics features for PyTorch MDAE model training (dataset and statistics).",
            "exec": "MDFeaturizer",
            "docs": "https://biobb-pytorch.readthedocs.io/en/latest/mdae.html#module-biobb_pytorch.mdae.mdfeaturizer",
            "rest": true
        },
        {
            "block": "TrainModel",
            "tool": "train_model",
            "desc": "Train a PyTorch autoencoder model (MDAE) from an input dataset and initial model, optionally saving the trained model and training metrics.",
            "exec": "trainModel",
            "docs": "https://biobb-pytorch.readthedocs.io/en/latest/mdae.html#module-biobb_pytorch.mdae.train_model",
            "rest": true
        },
        {
            "block": "EvaluateModel",
            "tool": "evaluate_model",
            "desc": "Evaluate a trained PyTorch autoencoder model (MDAE) on a dataset, computing loss and collecting model evaluation variables.",
            "exec": "evaluateModel",
            "docs": "https://biobb-pytorch.readthedocs.io/en/latest/mdae.html#module-biobb_pytorch.mdae.evaluate_model",
            "rest": true
        },
        {
            "block": "EvaluateEncoder",
            "tool": "encode_model",
            "desc": "Apply the encoder part of a trained MDAE model to a dataset, producing latent variables (collective variables).",
            "exec": "evaluateEncoder",
            "docs": "https://biobb-pytorch.readthedocs.io/en/latest/mdae.html#module-biobb_pytorch.mdae.encode_model",
            "rest": true
        },
        {
            "block": "EvaluateDecoder",
            "tool": "decode_model",
            "desc": "Apply the decoder part of a trained MDAE model to latent variables, reconstructing molecular dynamics features.",
            "exec": "evaluateDecoder",
            "docs": "https://biobb-pytorch.readthedocs.io/en/latest/mdae.html#module-biobb_pytorch.mdae.decode_model",
            "rest": true
        },
        {
            "block": "GeneratePlumed",
            "tool": "make_plumed",
            "desc": "Generate a PLUMED input file, a features.dat file and a TorchScript (.ptc) model from a trained MDAE model and feature statistics, suitable for biased molecular dynamics simulations.",
            "exec": "make_plumed",
            "docs": "https://biobb-pytorch.readthedocs.io/en/latest/mdae.html#module-biobb_pytorch.mdae.generate_plumed",
            "rest": true
        },
        {
            "block": "Feat2Traj",
            "tool": "feat2traj",
            "desc": "Convert reconstructed molecular dynamics features (e.g. decoded coordinates) into an MD trajectory file using topology information from the stats file and/or a topology file.",
            "exec": "feat2traj",
            "docs": "https://biobb-pytorch.readthedocs.io/en/latest/mdae.html#module-biobb_pytorch.mdae.feat2traj",
            "rest": true
        },
        {
            "block": "LRP",
            "tool": "LRP",
            "desc": "Perform Layer-wise Relevance Propagation on the encoder of a trained MDAE model to obtain global feature importance scores.",
            "exec": "LRP",
            "docs": "https://biobb-pytorch.readthedocs.io/en/latest/mdae.html#module-biobb_pytorch.mdae.explainability.LRP",
            "rest": true
        }
    ],
    "dep_pypi": [
        "install_requires=['biobb_common==5.2.0', 'torch', 'lightning', 'mlcolvar', 'mdtraj']",
        "python_requires='>=3.10'"
    ],
    "dep_conda": [
        "python >=3.10",
        "biobb_common ==5.2.0",
        "pytorch",
        "pytorch-lightning",
        "mlcolvar",
        "mdtraj"
    ],
    "keywords": [
        "pytorch",
        "machine learning",
        "deep learning",
        "autoencoder",
        "molecular dynamics",
        "neural networks",
        "MDAE"
    ]
}

